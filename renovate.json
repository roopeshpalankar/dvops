"regexManagers": [
    {
      "datasourceTemplate": "github-releases",
      "fileMatch": [
        "kpoint/.*\\.yaml"
      ],
      "matchStrings": [
        " +chartVersion: +(?<currentValue>[^'\" ]+) +# renovate: depName=(?argoproj/argo-helm[^ \\n]+) +(extractVersion=(?argo-cd[^\\n]+))?",
      ],
    },
  ]
}
