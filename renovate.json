  "regexManagers": [
    {
      "datasourceTemplate": "github-releases",
      "fileMatch": [
        "*.yaml"
      ],
      "matchStrings": [
        " +chartVersion: +(?<currentValue>[^'\" ]+) +# renovate: depName=(?<depName>[^ \\n]+) +(extractVersion=(?<extractVersion>[^\\n]+))?",
      ],
    },
  ]
